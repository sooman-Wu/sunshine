/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

page {
   background: #f2f2f2;
}
.advertisiment-banner {
  width: 100%;
}

.advertisiment-banner image {
  width: 100%;
}

.tab-h{
  height: 84rpx;width: 100%; box-sizing: border-box;overflow: hidden;line-height: 80rpx;background: #fff; font-size: 32rpx; white-space: nowrap;position: fixed;top: 0; left: 0; z-index: 99;}
.tab-item{padding:0 30rpx;display: inline-block; box-sizing: border-box;}
.tab-item.active{border-bottom: 2px solid #e50d02;}
.item-ans{ width: 100%;display: flex; flex-grow: row no-wrap;justify-content: space-between; padding: 30rpx;box-sizing: border-box; height: 180rpx;align-items: center;border-bottom: 1px solid #F2F2F2;}
.avatar{width: 100rpx;height: 100rpx;position: relative;padding-right: 30rpx;}
.avatar .img{width: 100%;height: 100%;}
.avatar .doyen{width: 40rpx;height: 40rpx;position: absolute;bottom: -2px;right: 20rpx;}
.expertInfo{font-size: 12px;flex-grow: 2;color: #B0B0B0;line-height: 1.5em;}
.expertInfo .name{font-size: 16px;color:#000;margin-bottom: 6px;}
.askBtn{ width: 120rpx;height: 60rpx;line-height: 60rpx;text-align: center;font-size: 14px; border-radius: 60rpx;border: 1px solid #4675F9; color:#4675F9;}
.tab-content{margin-top: 80rpx;}
.scoll-h{height: 100%;}

swiper {
  width: 100%;
  height: 180px;
}

swiper image{
  width: 100%;
}

.title {
  width: 100%;
  padding: 20rpx 0;
  font-size: 28rpx;
  font-weight: bold;
  color: #4BBFE8;
  text-align: center;
  background: #fff;
}

.scroll-box{
  width: 100%;
  height: 190rpx;
  white-space: nowrap;
  background: #fff;
  margin-bottom: 20rpx;
}
.scroll-view-item_H{
  width: 100%;
  height: 160rpx;
  padding: 0 50rpx;
  display: inline-block;
  box-sizing: border-box;
  white-space: normal;
}

.street-lis {
  width: calc(100%/3);
  margin-bottom: 10rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  color: #fff;
  float: left;
}

.street-lis navigator {
  display: block;
  width: 94%;
  margin: 0 auto;
  height: 100%;
  text-align: center;
  border-radius: 10rpx;
}

.section-scroll {
   position: relative;
}

.controll {
  width: 40rpx;
  background: none;
  border: 0;
  outline: none;
  position: absolute;
}

.controll-prev {
  left: 5rpx;
  top: 35%;
}

.controll-next {
  right: 5rpx;
  top: 35%;
}

.controll image {
   width: 100%;
}

.street-lis:nth-child(6n-5) navigator {
  background: #F9D133;
}

.street-lis:nth-child(6n-4) navigator {
  background: #F35C65;
}

.street-lis:nth-child(6n-3) navigator {
  background: #4BBFE8;
}

.street-lis:nth-child(6n-2) navigator {
  background: #A488F7;
}

.street-lis:nth-child(6n-1) navigator {
  background: #FCB452;
}

.street-lis:nth-child(6n) navigator {
  background: #7DD486;
}

.content-wrapper {
  width: 100%;
  padding: 20rpx;
  padding-top: 0;
  box-sizing: border-box;
  background: #fff;
  font-size: 28rpx;
  color: #333;
  letter-spacing: 4rpx;
  line-height: 40rpx;
}

